{
  "name": "maney-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier -w --config=.prettierrc.json index.html src",
    "typechecking": "vue-tsc --noEmit --project tsconfig.app.json",
    "gen-openapi": "openapi-ts -i openapi.yaml -o src/client -c @hey-api/client-axios",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@hey-api/client-axios": "^0.2.3",
    "element-plus": "^2.8.1",
    "pinia": "^2.2.2",
    "vue": "^3.4.37",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.53.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitest/coverage-v8": "^2.0.5",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^15.7.3",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.1",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^v18.13.0",
    "npm": "^8.19.3"
  }
}